n1 = float(input("Digite a primeira nota: "))
n2 = float(input("Digite a segunda nota: "))
n3 = float(input("Digite a terceira nota: "))

total = n1 + n2 + n3 
t = total / 3
print(t)
if t >= 7.0:
  print("Aprovado!")
elif t >= 5.0  and t <= 6.9:
  print("RecuperacÃ£o")
elif t < 5:
  print("Reprovado!")
